{
    "manifest_version": 3,
    "name": "My Password Generator",
    "version": "1.0",
    "description": "A simple password generator extension",
    "icons": {
      "48": "icon.png"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "500": "icon.png"
      }
    },
    "permissions": [
      "activeTab"
    ]
  }
  
